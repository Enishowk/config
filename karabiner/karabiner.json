{
    "global": { "show_in_menu_bar": false },
    "profiles": [
        {
            "complex_modifications": {
                "rules": [
                    {
                        "description": "Launch Finder by hyper+1.",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "1",
                                    "modifiers": { "mandatory": ["left_shift", "left_command", "left_control", "left_option"] }
                                },
                                "to": [{ "shell_command": "open ~/Downloads" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Launch Firefox by hyper+2.",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "2",
                                    "modifiers": { "mandatory": ["left_shift", "left_command", "left_control", "left_option"] }
                                },
                                "to": [{ "shell_command": "open '/Applications/Firefox Developer Edition.app'" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Launch Zed by hyper+3.",
